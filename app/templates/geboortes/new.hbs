{{!-- Page to create a new geboorte --}}
<div class="new">
  <h2 class="title">{{t (humanize 'New geboorte')}}</h2>

  {{!--
    Actions to save new geboorte
    or delete new geboorte.
  --}}
  <div class="actions">
    <button type="button" class="btn save" title="Save new geboorte" {{action 'save' model}}>Save</button>
    <button type="button" class="btn cancel" title="Cancel" {{action 'cancel' model}}>Cancel</button>
  </div>

  {{!-- Table to show details of geboorte. --}}
  <table>
    <thead>
      <tr>
        <th>Attribute</th>
        <th>Data</th>
      </tr>
    </thead>
    <tbody>
      {{!-- Creating rows for attributes depending on the type of property --}}
            <tr>
        <td>datum</td>
<td align='middle'>{{handle-date model=model attribute='datum' editable=true}}</td>
      </tr>
      
      
    </tbody>
  </table>
</div>
