{{!-- Page to create a new toezicht-nomenclature --}}
<div class="new">
  <h2 class="title">{{t (humanize 'New toezicht-nomenclature')}}</h2>

  {{!--
    Actions to save new toezicht-nomenclature
    or delete new toezicht-nomenclature.
  --}}
  <div class="actions">
    <button type="button" class="btn save" title="Save new toezicht-nomenclature" {{action 'save' model}}>Save</button>
    <button type="button" class="btn cancel" title="Cancel" {{action 'cancel' model}}>Cancel</button>
  </div>

  {{!-- Table to show details of toezicht-nomenclature. --}}
  <table>
    <thead>
      <tr>
        <th>Attribute</th>
        <th>Data</th>
      </tr>
    </thead>
    <tbody>
      {{!-- Creating rows for attributes depending on the type of property --}}
            <tr>
        <td>label</td>
<td align='middle'>{{handle-attribute model=model attribute='label' editable=true}}</td>
      </tr>
      <tr>
        <td>code</td>
<td align='middle'>{{handle-attribute model=model attribute='code' editable=true}}</td>
      </tr>
      
      
    </tbody>
  </table>
</div>
