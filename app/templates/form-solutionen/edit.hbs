{{!-- Page to edit form-solution --}}
{{#with (changeset model skipValidate=true) as |changeset|}}
<div class="edit">
  <h2 class="title">{{t (humanize 'Edit form-solution')}}</h2>

  {{!--
    Actions to save or cancel changes;
    or delete form-solution.
  --}}
  <div class="actions">
    <button type="button" class="btn save" title="Save new form-solution" {{action 'save' changeset model}}>Save</button>
    <button type="button" class="btn cancel" title="Cancel" {{action 'cancel' changeset model}}>Cancel</button>
    <button type="button" class="btn delete" title="Delete this item" {{action 'delete' changeset model}}>Delete</button>
  </div>

  {{!-- Table to show details of form-solution. --}}
  <table>
    <thead>
      <tr>
        <th>Property</th>
        <th>Data</th>
      </tr>
    </thead>
    <tbody>
      {{!-- Creating rows for attributes depending on the type of property --}}
            <tr>
      <td>hasOwner</td>
      <td align='middle'>{{handle-attribute model=changeset attribute='hasOwner' editable=true}}</td>
</tr>

        <tr><td>formNode</td><td align='middle'>{{edit-belongs-to relName=changeset.formNode relType="form-node"}}</td></tr>
      <tr><td>inzendingVoorToezicht</td><td align='middle'>{{edit-belongs-to relName=changeset.inzendingVoorToezicht relType="inzending-voor-toezicht"}}</td></tr>
        

    </tbody>
  </table>
</div>
{{/with}}
