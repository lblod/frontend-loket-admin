{{!-- Page to edit inzending-voor-toezicht --}}
{{#with (changeset model skipValidate=true) as |changeset|}}
<div class="edit">
  <h2 class="title">{{t (humanize 'Edit inzending-voor-toezicht')}}</h2>

  {{!--
    Actions to save or cancel changes;
    or delete inzending-voor-toezicht.
  --}}
  <div class="actions">
    <button type="button" class="btn save" title="Save new inzending-voor-toezicht" {{action 'save' changeset model}}>Save</button>
    <button type="button" class="btn cancel" title="Cancel" {{action 'cancel' changeset model}}>Cancel</button>
    <button type="button" class="btn delete" title="Delete this item" {{action 'delete' changeset model}}>Delete</button>
  </div>

  {{!-- Table to show details of inzending-voor-toezicht. --}}
  <table>
    <thead>
      <tr>
        <th>Property</th>
        <th>Data</th>
      </tr>
    </thead>
    <tbody>
      {{!-- Creating rows for attributes depending on the type of property --}}
            <tr>
      <td>created</td>
      <td align='middle'>{{handle-date model=changeset attribute='created' editable=true timePicker=true}}</td>
</tr>
      <tr>
      <td>modified</td>
      <td align='middle'>{{handle-date model=changeset attribute='modified' editable=true timePicker=true}}</td>
</tr>
      <tr>
      <td>sentDate</td>
      <td align='middle'>{{handle-date model=changeset attribute='sentDate' editable=true timePicker=true}}</td>
</tr>
      <tr>
      <td>description</td>
      <td align='middle'>{{handle-attribute model=changeset attribute='description' editable=true}}</td>
</tr>
      <tr>
      <td>remark</td>
      <td align='middle'>{{handle-attribute model=changeset attribute='remark' editable=true}}</td>
</tr>
      <tr>
      <td>temporalCoverage</td>
      <td align='middle'>{{handle-attribute model=changeset attribute='temporalCoverage' editable=true}}</td>
</tr>
      <tr>
      <td>businessIdentifier</td>
      <td align='middle'>{{handle-attribute model=changeset attribute='businessIdentifier' editable=true}}</td>
</tr>
      <tr>
      <td>businessName</td>
      <td align='middle'>{{handle-attribute model=changeset attribute='businessName' editable=true}}</td>
</tr>
      <tr>
      <td>nomenclature</td>
      <td align='middle'>{{handle-attribute model=changeset attribute='nomenclature' editable=true}}</td>
</tr>
      <tr>
      <td>dateOfEntryIntoForce</td>
      <td align='middle'>{{handle-date model=changeset attribute='dateOfEntryIntoForce' editable=true}}</td>
</tr>
      <tr>
      <td>endDate</td>
      <td align='middle'>{{handle-date model=changeset attribute='endDate' editable=true}}</td>
</tr>
      <tr>
        <td>{{t 'isModification'}}</td>
        <td>{{handle-attribute model=model attribute='isModification' classes='checkbox' editable=true boolean=true}}</td>
      </tr>
      <tr>
        <td>{{t 'hasExtraTaxRates'}}</td>
        <td>{{handle-attribute model=model attribute='hasExtraTaxRates' classes='checkbox' editable=true boolean=true}}</td>
      </tr>
      <tr>
      <td>agendaItemCount</td>
      <td align='middle'>{{handle-attribute model=changeset attribute='agendaItemCount' editable=true}}</td>
</tr>
      <tr>
      <td>sessionDate</td>
      <td align='middle'>{{handle-date model=changeset attribute='sessionDate' editable=true timePicker=true}}</td>
</tr>
      <tr>
      <td>title</td>
      <td align='middle'>{{handle-attribute model=changeset attribute='title' editable=true}}</td>
</tr>

        <tr><td>status</td><td align='middle'>{{edit-belongs-to relName=changeset.status relType="document-status"}}</td></tr>
      <tr><td>lastModifier</td><td align='middle'>{{edit-belongs-to relName=changeset.lastModifier relType="gebruiker"}}</td></tr>
      <tr><td>bestuurseenheid</td><td align='middle'>{{edit-belongs-to relName=changeset.bestuurseenheid relType="bestuurseenheid"}}</td></tr>
      <tr><td>formSolution</td><td align='middle'>{{edit-belongs-to relName=changeset.formSolution relType="form-solution"}}</td></tr>
      <tr><td>inzendingType</td><td align='middle'>{{edit-belongs-to relName=changeset.inzendingType relType="toezicht-inzending-type"}}</td></tr>
      <tr><td>besluitType</td><td align='middle'>{{edit-belongs-to relName=changeset.besluitType relType="besluit-type"}}</td></tr>
      <tr><td>bestuursorgaan</td><td align='middle'>{{edit-belongs-to relName=changeset.bestuursorgaan relType="bestuursorgaan"}}</td></tr>
        <tr><td>files</td><td align='middle'>{{edit-has-many relName=changeset.files relType="file"}}</td></tr>
      <tr><td>taxRates</td><td align='middle'>{{edit-has-many relName=changeset.taxRates relType="tax-rate"}}</td></tr>

    </tbody>
  </table>
</div>
{{/with}}
